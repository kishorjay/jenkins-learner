pipeline {
    agent any

    stages {
        stage('Started Pipe') {
            steps {
                echo 'intiated'
            }
        }
            
        stage('Build') {
			script {
			        def kj = ""
			        kj = bat(script : "echo hello", returnStdout: true)
				echo kj
			}
			steps {
				echo 'performing build stage'
				echo kj

			}
        }
		stage('Parellels') {
        parallel {
			stage('Test') {
				steps {
					echo 'performing test stage'
			}
			}
			stage('Deploy') {
				steps {
					echo 'performing deploy stage'
			}
			}
	} 
   }
  }
}
