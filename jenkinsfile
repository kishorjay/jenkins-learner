pipeline {
    agent any

    stages {
	    stage('Run Parellels'){
         parellel {
			stage('Test') {
				steps {
					echo 'performing test stage'
			}
			}
			stage('Deploy') {
				steps {
					echo 'performing deploy stage'
			}
			}
	} 
   }
        stage('Started Pipe') {
            steps {
                echo 'intiated'
            }
        }
            
        stage('Build') {
			steps {
				echo 'performing build stage'
			}
        }

  }
}
