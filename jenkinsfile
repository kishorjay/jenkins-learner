pipeline {
    agent any
    stages {
        stage('Run Tests') {
            parallel {
                stage('Test On Windows') {
                   
                    steps {
                        echo "wind"
                    }
                    post {
                        always {
                            echo "always post"
                        }
                    }
                }
                stage('Test On Win 2') {
                    
                    steps {
                        echo "wind1"
                    }
                    post {
                        always {
                            echo "always post1"
                        }
                    }
                }
            }
        }
    }
}
