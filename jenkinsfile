pipeline {
    agent any

    stages {
        stage('Started Pipe') {
            steps {
                echo 'intiated'
            }
        }
            
        stage('Build') {
			steps {
				echo 'performing build stage'
				//bat 'dir'
				def x = bat(
				    returnStdout: true,
				    script: "${dir}"
				)

				echo x
			}
        }
		stage('Parellels') {
        parallel {
			stage('Test') {
				steps {
					echo 'performing test stage'
			}
			}
			stage('Deploy') {
				steps {
					echo 'performing deploy stage'
			}
			}
	} 
   }
  }
}
